CREATE OR REPLACE FUNCTION usp_InsertTransportRequestE070L(IN pTRKORR VARCHAR(20), IN pLASTNUM VARCHAR(10))
RETURNS VOID AS $$
BEGIN
    START TRANSACTION;
        IF EXISTS(SELECT 1 FROM "E070L" WHERE "LASTNUM" = pLASTNUM)
        THEN
            UPDATE "E070L" SET "TRKORR" = pTRKORR WHERE "LASTNUM" = pLASTNUM; 
        ELSE
            INSERT INTO "E070L" ("TRKORR", "LASTNUM") VALUES (pTRKORR, pLASTNUM);
        END IF;
    COMMIT;
END;
$$ LANGUAGE plpgsql;
